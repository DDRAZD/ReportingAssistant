@model List<ReportingAssistant.DomainModels.ApplicationUser>


@{
    ViewBag.Title = "List of users";
}

<h2>Index</h2>

<table class="table table-bordered">
    <tr>
        <th>User Name</th>
        <th>Email</th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>@user.Email</td>
        </tr>
    }



</table>